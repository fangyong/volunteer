<?xml version="1.0" encoding="utf-8"?>
<com.baiduvolunteer.view.ListViewCell xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content" >

    <ImageView
        android:id="@+id/iconView"
        android:layout_width="44dip"
        android:layout_height="44dip"
        android:padding="5dp"
        android:layout_alignParentLeft="true"
        android:layout_centerVertical="true"
        android:layout_marginLeft="10dip" />

    <ImageView
        android:id="@+id/detailIcon"
        android:layout_width="18dip"
        android:layout_height="18dip"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        android:layout_marginRight="10dip" />

    <TextView
        android:id="@+id/textLabel"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_marginLeft="5dp"
        android:layout_toLeftOf="@+id/detailIcon"
        android:layout_toRightOf="@+id/iconView"
        android:gravity="center_vertical"
        android:text=""
        android:textColor="@color/gary"
        android:textSize="@dimen/text_size_20" >
    </TextView>

</com.baiduvolunteer.view.ListViewCell>